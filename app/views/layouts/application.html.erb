

<style>
    #maincontent {
        border-right-style: solid;
        border-right-color: #eee;
        border-width: 0.06cm;
        padding-left: 120px;
        margin-right: 0;

    }

    #sidecontent {
        padding-right: 120px;
    }
</style>

<!-- The html head declaration-->
<%= render "layouts/head"%>

<!-- The Header of website-->
<%= render "layouts/header2" %>

<!-- Begin Body -->
<div class="container-fluid">
  <div class="no-gutter row">
    <!-- left side column -->
    <%=
        #render 'layouts/navbar2'
    %>
    <!--/end left column-->
    <div class="col-xs-12 col-md-9" id="maincontent">
      <%= yield %>
    </div>
    <div class="col-xs-12 col-md-3" id="sidecontent">
      <%=
          render "layouts/side_content"
      %>
    </div>
  </div>
</div>
<!-- End of Body-->

<footer id="footer_div">
  <%= render 'layouts/footer_pub' %>

</footer>

<%= render "layouts/foot" %>
